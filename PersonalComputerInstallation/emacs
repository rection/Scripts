;; load emacs 24's package system. Add MELPA repository.

;; Added by Package.el.  This must come before configurations of
;; installed packages.  Don't delete this line.  If you don't want it,
;; just comment it out by adding a semicolon to the start of the line.
;; You may delete these explanatory comments.
(package-initialize)

(when (>= emacs-major-version 24)
  (require 'package)
  (add-to-list
   'package-archives
   ;; '("melpa" . "http://stable.melpa.org/packages/") ; many packages won't show if using stable
   '("melpa" . "http://melpa.milkbox.net/packages/")
   t))
(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(custom-enabled-themes (quote (atom-dark)))
 '(custom-safe-themes
   (quote
    ("2642a1b7f53b9bb34c7f1e032d2098c852811ec2881eec2dc8cc07be004e45a0" default)))
 '(package-selected-packages
   (quote
    (bash-completion python-mode python-django python-environment atom-dark-theme yaml-mode web-beautify web-search vagrant toml toml-mode todoist tiny tiny-menu pg jedi jedi-core jenkins google-translate markdown-changelog markdown-mode markdown-mode+ markdown-preview-eww markdown-preview-mode gitlab gitlab-ci-mode gitlab-ci-mode-flycheck magit flycheck-grammarly format-all flymake-ruby flymake-yaml flymake-go flycheck-clang-tidy flycheck-clang-analyzer auto-compile auto-complete auto-complete-c-headers auto-complete-clang))))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 )
